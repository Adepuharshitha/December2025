from typing import List

class Solution:
    def minDeletionSize(self, strs: List[str]) -> int:
        n = len(strs)
        m = len(strs[0])

        # dp[j] = longest valid sequence ending at column j
        dp = [1] * m

        for j in range(m):
            for i in range(j):
                # check if column i can come before column j for all rows
                if all(strs[row][i] <= strs[row][j] for row in range(n)):
                    dp[j] = max(dp[j], dp[i] + 1)

        # minimum deletions = total columns - maximum kept columns
        return m - max(dp)
